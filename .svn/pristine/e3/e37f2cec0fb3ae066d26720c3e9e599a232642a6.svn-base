<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sb.framework"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sb.framework.test.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sb.framework.test.TextViewActivity"
            android:label="@string/title_activity_text_view" >
        </activity>
        <activity
            android:name="com.sb.framework.test.ImageViewActivity"
            android:label="@string/title_activity_image_view" >
        </activity>
        <activity
            android:name="com.sb.framework.test.RubbishOOMActivity"
            android:label="@string/title_activity_rubbish_oom" >
        </activity>
        <activity
            android:name="com.sb.framework.test.ListViewNormalActivity"
            android:label="@string/title_activity_adapter_view" >
        </activity>
        <activity
            android:name="com.sb.framework.test.HttpTestActivity"
            android:label="@string/title_activity_http_test_urlconnection" >
        </activity>
    </application>

</manifest>